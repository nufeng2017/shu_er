<!-- 购物车列表项 -->
<view class="z-shop-car-item">
  <van-checkbox value="{{ checkAll }}" bind:change="change" checked-color="#b20a0a" icon-size="40rpx" />
  <view class="z-shop-car-item-main">
    <view class="z-shop-car-item-title"> 
      <text>一次性</text> 
      <view class="z-delete-icon"><image src="{{iconUrl}}"/></view>
    </view>
    <view class="z-select-shop-item">常规服务</view>
    <view class="z-price ~z-flex">
      <text class="~c-ff3e3e">￥89.00</text>
      <view><van-stepper value="{{ 1 }}" bind:change="onChange" /></view>
    </view>
  </view>
</view>
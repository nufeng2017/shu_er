.z-main{
  padding-bottom: 140rpx;
}
.z-inf-box {
  border-bottom: 20rpx solid #f5f5f5;

}
.z-inf-box .z-inf-body{
  padding: 47rpx 30rpx 30rpx;
}
.z-address .z-shop-car-item .z-shop-car-item-main{
  padding-left: 60rpx;
}
.z-address .z-shop-car-item .z-shop-car-item-main .z-shop-car-item-img{
  width:40rpx;
  height: 40rpx;
  border-radius: 0;
  border: none;
}
.z-address .z-shop-car-item .z-shop-car-item-main .z-shop-car-item-title{
  color:#212121;
  font-weight: 900;
  font-size: 28rpx;
}
.z-address-info{
  line-height: 40rpx;
  color:#333;
  font-size: 22rpx;
}
.z-checkbox-title,.z-address-btns{
  font-size: 30rpx;
}
.z-address-btns{
  height: 100rpx;
}
.z-address-btns>view{
  margin-left: 30rpx;
  height: 100%;
  padding-left: 42rpx;
  position: relative;
  line-height: 100rpx;
}
.z-address-btns>view::before{
  content: '';
  position: absolute;
  height: 100%;
  width:33rpx;
  background: url(data:image/png;base64,) no-repeat center;
  background-size: 100% auto;
  top:0;
  left: 0;
}
.z-address-btns>view.z-delete::before{
  background-image: url(data:image/png;base64,)
}
.z-btn-box{
  position: fixed;
  width:100%;
  height: 140rpx;
  left: 0;
  bottom: 0;
  background: #fff;
  box-sizing: border-box;
  padding: 27rpx 50rpx 0;
}
.z-btn-box .z-btn{
  width:100%;
}
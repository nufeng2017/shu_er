.z-banner{
  height: 257rpx;
}
.z-main-box{
  padding: 50rpx;
}
.z-main-box .z-block-title{
  font-size: 32rpx;
  line-height: 1;
  margin-bottom: 24rpx;
}
.z-main-box .z-last-money{
  font-size: 75rpx;
  color:#ff3333;
  margin-bottom: 55rpx;
}
.z-main-box .z-last-money text{
  font-size: 30rpx;
  margin-right: 5rpx;
}
.z-sel-item-box{
  padding-top: 20rpx;
}
.z-sel-item-box .z-rechage-item{
  box-sizing: border-box;
  width:200rpx;
  height: 140rpx;
  border-radius: 10rpx;
  border:1px solid #e1e1e1;
  margin-right: 25rpx;
  margin-bottom: 36rpx;
  font-size: 50rpx;
  line-height: 1;
  position: relative;
}
.z-sel-item-box .z-rechage-item:nth-child(3n){
  margin-right: 0px;
}
.z-sel-item-box .z-price text{
  font-size: 26rpx;
}
.z-sel-item-box .z-gift{
  color:#ff6666;
  font-size: 24rpx;
  margin-top: 10rpx;
}
.z-sel-item-box .z-recommend:after{
  content: '';
  background: url(data:image/png;base64,) no-repeat center;
  position: absolute;
  right: -13rpx;
  top:-14rpx;
  background-size:100%;
  width:67rpx;
  height: 29rpx;
}
.z-sel-this{
  border-color:#ff6666 !important;
  background: #fff4f4;
  color:#ff3333;
}
.z-submit-bar-container .z-submit-bar-btn{
  width:300rpx !important;
}
.z-other-num{
  margin-top: 15rpx;
}
.z-input-box{
  padding:10rpx 0 200rpx;
}
.z-input-box input{
  display: inline-block;
  border-bottom: 1px solid #ccc;
  width:200rpx;
  vertical-align: text-bottom;
  font-size: 50rpx;
}
.z-page-head{
  width:100%;
  height: 352rpx;
  position: relative;
}
.z-member-info-box{
  position: absolute;
  height: 128rpx;
  width:100%;
  left: 0;
  top:53rpx;
  box-sizing: border-box;

}
.z-member-info-box .z-img-box{
  margin: 0 30rpx;
  width:128rpx;
  height: 128rpx;
  border-radius: 50%;
  overflow: hidden;
  position:  relative;
}
.z-member-info-box .z-img-box open-data{
  position: absolute;
  width:100%;
  height: 100%;
  left: 0;
  top:0;
}
.z-member-info-box .z-member-num{
  color:#fff;
  font-size: 34rpx;
  width:100%;
}
.z-member-info-box .z-member-num view{
  padding: 0 12rpx;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 22rpx;
  background: #d35353;
  border-radius: 50px;
  margin-left: 6rpx;
}
.z-member-info-box .z-last-money{
  font-size:24rpx;
  color:#fe9999;
  width:100%;
  margin-top: 14rpx;
}
.z-member-info-box .z-last-money text{
  font-size: 30rpx;
  color:#fec2c2;
  margin-left: 5rpx;
}
.z-member-code{
  width:122rpx;
  height: 104rpx;
  position: absolute;
  right: 0;
  top:65rpx;
  z-index: 100;
}
.z-member-code .z-grid-container{
  height: 100%;
  width:74rpx;
}
.z-member-code .z-grid-title{
  font-size: 24rpx;
  color:#fff;
  margin-top: 14rpx;
}

/* 立即充值 */
.z-add-money-box{
  position: absolute;
  width:100%;
  height: 123rpx;
  left: 0;
  bottom: 0;
  font-size: 24rpx;
  color:#fac04e;
}
.z-add-money-box .z-add-money-tips image{
  width:57rpx;
  height: 39rpx;
  display: inline-block;
  margin-right: 10rpx;
}
.z-add-money-box .z-add-money-btn{
  height: 58rpx;
  width:160rpx;
  color:#fff;
  background: #e4ac3f;
  line-height: 58rpx;
  text-align: center;
  border-radius: 50px;
}

/* 单元格 */
.z-cell-box{
  padding-top: 30rpx;
}
.z-log-out{
  padding: 22rpx 50rpx 30rpx;
}
.z-log-out-btn{
  border:1px solid #b20a0a;
  color:#b20a0a;
  width:100%;
}
.z-cell-text{
  color:#333;
}
.z-cell{
  background: #fff !important;
}
/* 客服按钮 */
.z-ke-fu{
  background: #fff;
  border: none;
  outline: none;
  padding: 0;
  height: 100%;
  width:100%;
  display: block;
}

.z-ke-fu:after,.z-ke-fu::before{
  display: none;
}
.z-ke-fu .z-btn-text{
  font-size: 28rpx;
  color:#333;
  font-weight: 400;
  text-align: left;
  background: #fff;
}
/* 弹窗 */
.z-popup-content{
  height: 160rpx;
  color:#5a5a5a;
  font-size: 36rpx;
  line-height: 160rpx;
  text-align: center;
}
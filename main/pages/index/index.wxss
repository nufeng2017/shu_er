/* main/pages/index/index.wxss */
.z-main{
  background: url(data:image/png;base64,) no-repeat left top;
  background-size: 100% auto;
  padding-top: 1px;
}
.z-view-box{
  margin: 0 45rpx;
}
.z-grid-box{
  height: 190rpx;
  width:220rpx;
  position: relative;
}
.z-login-view{
  position: absolute;
  left:0;
  height:100%;
  width:100%;
  top:0;
}
.z-index-head-link .z-grid-title{
  color:#fff;
}
.z-shop-info{
  color:#fff;
  margin-top: 26rpx;
  height: 38rpx;
  line-height: 38rpx;
  padding-left: 82rpx;
  position: relative;
  font-size: 34rpx;
  display: inline-block;
}
.z-shop-info:before{
  content: '';
  width:30rpx;
  height: 38rpx;
  background: url(data:image/png;base64,) no-repeat center;
  background-size: 100%;
  position: absolute;
  top:0;
  left: 35rpx;
}
.z-shop-address-info{
  display: inline-block;
}
.z-shop-address-info .z-shop-address{
  color:#d4bdb1;
  font-size: 24rpx;
}
.z-shop-address-info .z-shop-address::before{
  content: '';
  display: inline-block;
  height: 24rpx;
  width:1px;
  background: #d4bdb1;
  margin: 0 18rpx;
  vertical-align: -10%;
}

/* 轮播 */
.z-swiper-container{
  height: 262rpx;
}
.z-swiper-wrap,.z-swiper-img-box,.z-swiper-img-container{
  height: 100%;
}
.z-swiper-img-box{
  padding: 0 45rpx;
}
.z-swiper-img-container{
  border-radius: 10rpx;
  height: 242rpx;
  overflow: hidden;
  box-sizing: border-box;
  background: #fff;
  box-shadow: 0 4rpx 10rpx 4rpx rgba(0,0,0,0.3);
}
.wx-swiper-dots{
  margin-bottom:20rpx;
}
.z-pro-link grid{
  width:123rpx;
  height: 210rpx;
}
.z-pro-link grid .z-grid-container{
  width:100%;
  height: 100%;
}
.z-pro-link .z-grid-container .z-icon-box{
  width:123rpx;
  height: 124rpx;
}

/* 热销产品 */
.z-hot-production{
  padding-top: 50rpx;
}
.z-hot-production .z-check-more{
  color:#000;
  font-size: 40rpx;
  font-weight: 900;
}
.z-hot-production .z-check-more .c-999999{
  font-size: 26rpx;
  line-height: 70rpx;
  font-weight: 400;
}
.z-shop-box{
  margin-top: 15rpx;
}
.z-shop-box .z-shop-item{
  margin-right: 40rpx;
  margin-bottom: 40rpx;
}
.z-shop-box .z-shop-item:nth-child(2n){
  margin-right: 0;
}
.z-add-adress-tips{
 font-size: 30rpx;
 text-align: center;
 line-height: 50rpx;
 padding: 50rpx;
}